{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@root/*": [
        "*"
      ],
      "@config/*": [
        "src/config/*"
      ],
      "@controllers/*": [
        "src/controllers/*"
      ],
      "@mail/*": [
        "src/mail/*"
      ],
      "@middlewares/*": [
        "src/middlewares/*"
      ],
      "@migrations/*": [
        "src/migrations/*"
      ],
      "@models/*": [
        "src/models/*"
      ],
      "@notification/*": [
        "src/notification/*"
      ],
      "@rests/*": [
        "src/rests/*"
      ],
      "@routes/*": [
        "src/routes/*"
      ],
      "@seeders/*": [
        "src/seeders/*"
      ],
      "@swagger/*": [
        "src/swagger/*"
      ],
      "@test/*": [
        "src/test/*"
      ],
      "@uploads/*": [
        "src/uploads/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@validations/*": [
        "src/validations/*"
      ]
    }
  },
  "include": [
    "src/**/*"
  ]
}